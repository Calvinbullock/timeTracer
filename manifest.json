{
    "manifest_version": 3,
    "name": "timeTracer",
    "description": "Notify a callback URL whenever a webpage is loaded or displayed",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    }
}

